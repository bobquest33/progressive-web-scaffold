{
  "name": "progressive-web-scaffold",
  "repository": {
    "type": "git",
    "url": "http://github.com/mobify/progressive-web-scaffold"
  },
  "private": true,
  "version": "0.0.1",
  "dependencies": {
    "es6-promise": "3.2.1",
    "immutable": "3.8.1",
    "lodash.debounce": "4.0.6",
    "lodash.isequal": "4.2.0",
    "lodash.times": "4.2.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.5.2",
    "react-router-scroll": "0.2.0",
    "redux": "3.5.2",
    "redux-act": "0.5.0",
    "redux-thunk": "2.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-istanbul": "1.0.3",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.9.0",
    "bluebird": "3.4.1",
    "chalk": "1.1.3",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.0.1",
    "eslint-import-resolver-webpack": "0.3.2",
    "eslint-plugin-ava": "2.5.0",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-react": "5.2.2",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-to-json": "0.6.0",
    "imports-loader": "0.6.5",
    "ip": "1.1.3",
    "json-loader": "0.5.4",
    "lodash.assign": "4.0.9",
    "lodash.template": "4.2.5",
    "minimist": "1.2.0",
    "mobify-code-style": "2.5.0",
    "nyc": "7.0.0",
    "progressive-web-sdk": "mobify/progressive-web-sdk.git#master",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.2.1",
    "react-hot-loader": "1.3.0",
    "react-styleguidist": "3.0.0",
    "rimraf": "2.5.3",
    "sass-loader": "4.0.0",
    "tap-mocha-reporter": "0.0.25",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "scripts": {
    "add:page": "node ./generators/add-page.js",
    "build": "webpack --config webpack/production.js -p --display-error-details --optimize-dedupe",
    "configure": "node ./generators/setup/configure.js",
    "dev": "node ./dev-server/index.js",
    "lint": "eslint '**/*.{js,jsx}'; exit 0",
    "test": "ava",
    "test:coverage": "cross-env NODE_ENV=test nyc ava",
    "test:watch": "npm test -- --watch",
    "styleguide": "styleguidist server"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true,
    "files": [
      "**/*.test.js",
      "!generators/page-skeleton/**"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "sourceMap": false,
    "instrument": false
  }
}
